{
  "name": "remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "concurrently -c blue,red \"pnpm:build:*\"",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "concurrently -c blue,red \"pnpm:dev:*\"",
    "dev:remix": "remix dev --manual -c \"tsx --inspect server.ts\"",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node --require dotenv/config ./build/server.js",
    "prod": "pnpm build && pnpm start",
    "typecheck": "tsc",
    "generate": "openapi-typescript ../../packages/api/docs/swagger.yaml -o ./types/schema.ts",
    "tunnel": "cloudflared tunnel --url http://localhost:3000"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "2.0.18",
    "@mantine/carousel": "7.2.2",
    "@mantine/core": "7.2.2",
    "@mantine/dates": "7.2.2",
    "@mantine/form": "7.2.2",
    "@mantine/hooks": "7.2.2",
    "@mantine/modals": "7.2.2",
    "@remix-run/css-bundle": "2.3.1",
    "@remix-run/express": "2.3.1",
    "@remix-run/node": "2.3.1",
    "@remix-run/react": "2.3.1",
    "@sentry/remix": "7.81.1",
    "@tabler/icons-react": "2.42.0",
    "@tanstack/react-query": "5.8.7",
    "@tanstack/react-table": "8.10.7",
    "chokidar": "3.5.3",
    "clsx": "2.0.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dayjs": "1.11.10",
    "embla-carousel-react": "7.1.0",
    "express": "4.18.2",
    "isbot": "3.7.1",
    "morgan": "1.10.0",
    "nanoid": "5.0.3",
    "nprogress": "0.2.0",
    "openapi-fetch": "0.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.2.0",
    "react-image-lightbox": "5.1.4",
    "react-infinite-scroll-component": "6.1.0",
    "react-lazy-load-image-component": "1.6.0",
    "react-textarea-autosize": "^8.5.3",
    "source-map-support": "0.5.21",
    "use-dehydrated-state": "0.1.0",
    "yet-another-react-lightbox": "3.15.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "2.3.1",
    "@remix-run/eslint-config": "2.3.1",
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@types/compression": "1.7.5",
    "@types/cookie": "0.5.4",
    "@types/eslint": "8.44.7",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "20.10.0",
    "@types/nprogress": "0.2.3",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@types/react-lazy-load-image-component": "1.6.3",
    "@types/source-map-support": "0.5.10",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "cookie": "0.6.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.7",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "openapi-typescript": "6.7.1",
    "postcss": "8.4.31",
    "postcss-preset-mantine": "1.11.0",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "remix-flat-routes": "0.6.2",
    "tailwindcss": "3.3.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.5.0",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
